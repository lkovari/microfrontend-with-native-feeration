{
  "name": "micro-fe-workspace",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "start-b": "ng serve microfrontend-b -o",
    "start-a": "ng serve microfrontend-a -o",
    "start-s": "ng serve shell -o",
    "build:shared-lib": "ng build shared-lib --configuration production",
    "toshared-lib": "cd dist/shared-lib",
    "publishshared-lib": "npm publish --access=public",
    "predeploy": "npm run build:prod",
    "gitdeploy": "gh-pages -d dist",
    "deployghpagesshell": "gh-pages -d projects/shell/dist/shell",
    "deployghpagesmfa": "gh-pages -d projects/microfrontend-a/dist/microfrontend-a",
    "deployghpagesmfb": "gh-pages -d projects/microfrontend-b/dist/microfrontend-b",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/native-federation": "^17.1.7",
    "@angular/animations": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "bootstrap": "^5.3.3",
    "es-module-shims": "^1.5.12",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.3",
    "@angular/cli": "^17.1.3",
    "@angular/compiler-cli": "^17.1.0",
    "@types/jasmine": "~5.1.0",
    "gh-pages": "^6.1.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "typescript": "~5.3.2"
  }
}
